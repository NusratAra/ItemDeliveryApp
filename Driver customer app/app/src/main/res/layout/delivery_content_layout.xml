<androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    android:id="@+id/track_layout">


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="36dp"
        android:paddingTop="16dp"
        android:background="@drawable/custom_linear_layout"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="20dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="9"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/trip_id"
                android:textColor="@color/colorGrayDark"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:background="@drawable/custom_button_white_border"
                android:fontFamily="@font/poppins_regular"
                android:gravity="center"
                android:paddingRight="20dp"
                android:text="@string/time_text"
                android:textAlignment="center"
                android:textColor="@color/colorGrayDark"
                android:textSize="14sp"
                tools:ignore="RtlCompat" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/history_id"
                android:textColor="@color/colorBlackLight"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/history_id"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:background="@drawable/custom_button_white_border"
                android:fontFamily="@font/poppins_regular"
                android:gravity="center"
                android:paddingRight="20dp"
                android:text="@string/date4"
                android:textAlignment="textEnd"
                android:textColor="@color/colorBlackLight"
                android:textSize="14sp"
                tools:ignore="RtlCompat" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/status_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_regular"
                android:text="@string/status"
                android:textColor="@color/colorGrayDark"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:background="@drawable/custom_button_white_border"
                android:fontFamily="@font/poppins_regular"
                android:gravity="center"
                android:paddingRight="20dp"
                android:text="@string/complete"
                android:textAlignment="textEnd"
                android:textColor="@color/colorGreen"
                android:textSize="14sp"
                tools:ignore="RtlCompat" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_margin="20dp"
            android:background="@drawable/divider" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:orientation="horizontal">

            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:src="@drawable/address_icon"
                fancy:civ_border_color="@android:color/transparent"
                fancy:civ_border_width="0dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:background="@drawable/custom_button_white_border"
                android:fontFamily="@font/poppins_regular"
                android:gravity="center"
                android:paddingTop="4dp"
                android:paddingRight="20dp"
                android:paddingBottom="4dp"
                android:text="@string/earn"
                android:textAlignment="textEnd"
                android:textColor="@color/colorMagentaDark"
                android:textSize="14sp"
                tools:ignore="RtlCompat" />

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/custom_button_white_border"
            android:fontFamily="@font/poppins_medium"
            android:gravity="start"
            android:text="@string/company_name"
            android:textAlignment="textStart"
            android:textColor="@color/colorBlackLight"
            android:textSize="14sp"
            tools:ignore="RtlCompat" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/custom_button_white_border"
            android:fontFamily="@font/poppins_regular"
            android:gravity="start"
            android:text="@string/demo_address"
            android:textAlignment="textStart"
            android:textColor="@color/colorGrayDark"
            android:textSize="14sp"
            tools:ignore="RtlCompat" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_margin="20dp"
            android:background="@drawable/divider" />

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:background="@drawable/custom_red_layout">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:fontFamily="@font/poppins_regular"
                android:gravity="center_vertical"
                android:paddingLeft="30dp"
                android:paddingTop="4dp"
                android:paddingBottom="4dp"
                android:text="@string/pick_up_point"
                android:textAlignment="center"
                android:textColor="@color/colorWhite"
                android:textSize="14sp"
                tools:ignore="RtlCompat" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:paddingLeft="20dp"
                android:src="@drawable/item_icon1"
                android:tint="@color/colorWhite" />

        </FrameLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/custom_button_white_border"
            android:fontFamily="@font/poppins_medium"
            android:gravity="start"
            android:text="@string/demo_address2"
            android:textAlignment="textStart"
            android:textColor="@color/colorBlackLight1"
            android:textSize="14sp"
            tools:ignore="RtlCompat" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:orientation="horizontal"
            android:weightSum="2">

            <ImageButton
                android:id="@+id/phone2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@drawable/custom_green_layout_phone"
                android:gravity="center"
                android:padding="10dp"
                android:src="@drawable/phone_icon" />


            <ImageButton
                android:id="@+id/procced2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@drawable/custom_green_layout_direct"
                android:gravity="center"
                android:padding="10dp"
                android:src="@drawable/procceed_icon" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_margin="20dp"
            android:background="@drawable/divider" />

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:background="@drawable/custom_green_layout_delivery_point">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:fontFamily="@font/poppins_regular"
                android:gravity="center_vertical"
                android:paddingLeft="30dp"
                android:paddingTop="4dp"
                android:paddingBottom="4dp"
                android:text="@string/delivery_point"
                android:textAlignment="center"
                android:textColor="@color/colorWhite"
                android:textSize="14sp"
                tools:ignore="RtlCompat" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:paddingLeft="20dp"
                android:src="@drawable/item_icon2"
                android:tint="@color/colorWhite" />

        </FrameLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/custom_button_white_border"
            android:fontFamily="@font/poppins_medium"
            android:gravity="start"
            android:text="@string/name"
            android:textAlignment="textStart"
            android:textColor="@color/colorBlackLight1"
            android:textSize="14sp"
            tools:ignore="RtlCompat" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="4dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/custom_button_white_border"
            android:fontFamily="@font/poppins_medium"
            android:gravity="start"
            android:text="@string/demo_address2"
            android:textAlignment="textStart"
            android:textColor="@color/colorBlackLight1"
            android:textSize="14sp"
            tools:ignore="RtlCompat" />

        <LinearLayout
            android:id="@+id/call_redirect_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="36dp"
            android:orientation="horizontal"
            android:weightSum="2">

            <ImageButton
                android:id="@+id/phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@drawable/custom_green_layout_phone"
                android:gravity="center"
                android:padding="10dp"
                android:src="@drawable/phone_icon" />


            <ImageButton
                android:id="@+id/procced"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@drawable/custom_green_layout_direct"
                android:gravity="center"
                android:padding="10dp"
                android:src="@drawable/procceed_icon" />
        </LinearLayout>


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>